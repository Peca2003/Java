public class Main
{
	public static void main(String[] args)
	{
		// Задание 1
		System.out.println("Задание 1");
		int[] array = { 33, 1, 3, 67, 97, 53, 71, 29, 15, 43, 35, 2, 65 };
		System.out.print(array[0]);
		for (int i = 1; i < array.length; i++)
		{
		    System.out.print(", " + array[i]);
		}
	}
}
